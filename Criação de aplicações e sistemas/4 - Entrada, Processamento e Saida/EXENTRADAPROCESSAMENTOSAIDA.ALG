Algoritmo "ExEntradaProcessamentoSaida"

Var
   // Seção de Declarações das variáveis
   num1: real
   num2: real
   resultado: real

Inicio
   Escreval("Digite o primeiro núnemo: ")
   Leia(num1)
   Escreval("Digite o segundo núnemo: ")
   Leia(num2)

   resultado <- num1 * num2
   Escreval("Resultado: ", resultado)


Fimalgoritmo